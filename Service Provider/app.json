{
  "expo": {
    "name": "ServiceProvider",
    "slug": "ServiceProvider",
    "privacy": "public",
    "icon": "./assets/service-provider-icon.png",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "androidStatusBar": {
      "backgroundColor": "#0000FF"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.service.provider"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.service.provider"
    }
  }
}